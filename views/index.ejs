<!DOCTYPE html>
<html lang="fr">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/CSS/index.css">
    <title>Dictionnaire</title>
</head>
<body>
    <h1>Dictionnaire</h1>

    <ul class="links">
        <a id="btn1" href="/enregistrement">Enregistrer une expression</a>
    </ul>

    <section id='expression'>
        <% dictionnaire.forEach((item, index)=> { %>
            <a href="/details/<%= index %>">
                <img class= "img2" src="<%= item.image %>" alt="">
                <h2>
                    <%= item.expression %>
                </h2>
            </a>
            <% }) %>
</section>
</body>
</html>